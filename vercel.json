{
  "rewrites": [
    { 
      "source": "/((?!.*\\.svg$).*)", 
      "destination": "/index.html" 
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.svg)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

